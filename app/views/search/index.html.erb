All the LPG and ELEC stations closest to <%= @zip %>


<% @stations.each do |station| %>
<ul>
  Name: <%= station.name %></br>
  Address: <%= station.address %></br>
  Types of Fuel: <%= station.fuel_type %></br>
  Distance From You: <%= station.distance %></br> Miles
  When You Can Access Them: <%= station.access_times %></br>
</ul>
<% end %>
